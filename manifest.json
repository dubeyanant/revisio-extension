{
  "manifest_version": 2,
  "name": "URL Grabber",
  "version": "2.0",
  "description": "Fetch URLs",
  "permissions": ["activeTab", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
