{
  "name": "URL Grabber",
  "manifest_version": 3,
  "description": "An extension that grabs and send URL to database",
  "version": "1.3.0",
  "author": "Anant Dubey",
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "URL Grabber",
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png"
  }
}
